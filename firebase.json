{
  "hosting": [{
    "source": ".",
    "frameworksBackend": {
      "region": "us-central1"
    },
    "headers": [
      {
        "source": "/tonconnect-manifest.json",
        "headers": [
          { "key": "Content-Type", "value": "application/json" },
          { "key": "Access-Control-Allow-Origin", "value": "*" }
        ]
      }
    ]
  }],
  "firestore": {
    "rules": "firestore.rules",
    "indexes": "firestore.indexes.json"
  }
}